language ESSENCE' 1.0

find x_ExplicitVarSizeWithFlags_Flags: matrix indexed by [int(1..4)] of bool
find x_ExplicitVarSizeWithFlags_Values: matrix indexed by [int(1..4)] of int(2..5)
find y_Occurrence: matrix indexed by [int(2..5)] of bool
branching on [x_ExplicitVarSizeWithFlags_Flags, x_ExplicitVarSizeWithFlags_Values, y_Occurrence]
such that
    and([x_ExplicitVarSizeWithFlags_Flags[q7] /\ y_Occurrence[j] -> x_ExplicitVarSizeWithFlags_Values[q7] + 2 = j
             | q7 : int(1..4), j : int(2..5)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q1 + 1] ->
         [x_ExplicitVarSizeWithFlags_Values[q1]; int(1)] <lex [x_ExplicitVarSizeWithFlags_Values[q1 + 1]; int(1)]
             | q1 : int(1..3)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q2] = false -> x_ExplicitVarSizeWithFlags_Values[q2] = 2 | q2 : int(1..4)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q3 + 1] -> x_ExplicitVarSizeWithFlags_Flags[q3] | q3 : int(1..3)]),
    1 <= sum([toInt(x_ExplicitVarSizeWithFlags_Flags[q4]) | q4 : int(1..4)]),
    1 <= sum([toInt(y_Occurrence[q6]) | q6 : int(2..5)])

