language ESSENCE' 1.0

find x_Occurrence: matrix indexed by [int(2..5)] of bool
find y_Occurrence: matrix indexed by [int(2..5)] of bool
find y_ExplicitVarSizeWithMarker_Marker: int(0..4)
find y_ExplicitVarSizeWithMarker_Values: matrix indexed by [int(1..4)] of int(2..5)
branching on [x_Occurrence, y_ExplicitVarSizeWithMarker_Marker, y_ExplicitVarSizeWithMarker_Values, y_Occurrence]
such that
    and([x_Occurrence[i] /\ y_Occurrence[j] -> i + 2 = j | i : int(2..5), j : int(2..5)]),
    1 <= sum([toInt(x_Occurrence[q1]) | q1 : int(2..5)]),
    1 <= sum([toInt(y_Occurrence[q2]) | q2 : int(2..5)]),
    and([q3 + 1 <= y_ExplicitVarSizeWithMarker_Marker ->
         y_ExplicitVarSizeWithMarker_Values[q3] < y_ExplicitVarSizeWithMarker_Values[q3 + 1]
             | q3 : int(1..3)]),
    and([q4 > y_ExplicitVarSizeWithMarker_Marker -> y_ExplicitVarSizeWithMarker_Values[q4] = 2 | q4 : int(1..4)]),
    1 <= y_ExplicitVarSizeWithMarker_Marker,
    y_ExplicitVarSizeWithMarker_Marker = sum([toInt(y_Occurrence[q6]) | q6 : int(2..5)]),
    y_ExplicitVarSizeWithMarker_Marker <= sum([toInt(y_Occurrence[q8]) | q8 : int(2..5)]),
    and([q9 <= y_ExplicitVarSizeWithMarker_Marker -> y_Occurrence[y_ExplicitVarSizeWithMarker_Values[q9]]
             | q9 : int(1..4)]),
    sum([toInt(y_Occurrence[q13]) | q13 : int(2..5)]) <= y_ExplicitVarSizeWithMarker_Marker,
    and([y_Occurrence[q10] ->
         or([q12 <= y_ExplicitVarSizeWithMarker_Marker /\ y_ExplicitVarSizeWithMarker_Values[q12] = q10
                 | q12 : int(1..4)])
             | q10 : int(2..5)])

