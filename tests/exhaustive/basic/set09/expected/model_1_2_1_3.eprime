    1 <= sum([toInt(x_Occurrence[q1]) | q1 : int(2..5)]),
    and([y_ExplicitVarSizeWithDummy[q2] < y_ExplicitVarSizeWithDummy[q2 + 1] \/ y_ExplicitVarSizeWithDummy[q2] = 6
