    and([y_ExplicitVarSizeWithDummy[q17] != 6 -> y_Occurrence[y_ExplicitVarSizeWithDummy[q17]] | q17 : int(1..4)]),
    and([y_Occurrence[q18] ->
         or([y_ExplicitVarSizeWithDummy[q20] != 6 /\ y_ExplicitVarSizeWithDummy[q20] = q18 | q20 : int(1..4)])
             | q18 : int(2..5)])

