    y_ExplicitVarSizeWithMarker_Marker <= sum([toInt(y_Occurrence[q15]) | q15 : int(2..5)]),
    and([q16 <= y_ExplicitVarSizeWithMarker_Marker -> y_Occurrence[y_ExplicitVarSizeWithMarker_Values[q16]]
