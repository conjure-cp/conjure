    sum([toInt(x_Occurrence[q1]) | q1 : int(1..2)]) <= 2,
    x_ExplicitVarSizeWithFlags_Flags[2] -> x_ExplicitVarSizeWithFlags_Values[1] < x_ExplicitVarSizeWithFlags_Values[2],
    and([x_ExplicitVarSizeWithFlags_Flags[q3] = false -> x_ExplicitVarSizeWithFlags_Values[q3] = 1 | q3 : int(1..2)]),
    x_ExplicitVarSizeWithFlags_Flags[2] -> x_ExplicitVarSizeWithFlags_Flags[1],
    sum([toInt(x_ExplicitVarSizeWithFlags_Flags[q5]) | q5 : int(1..2)]) <= 2,
    sum([toInt(x_ExplicitVarSizeWithFlags_Flags[q8]) | q8 : int(1..2)]) =
    sum([toInt(x_Occurrence[q9]) | q9 : int(1..2)]),
    sum([toInt(x_ExplicitVarSizeWithFlags_Flags[q13]) | q13 : int(1..2)]) <=
    sum([toInt(x_Occurrence[q14]) | q14 : int(1..2)]),
    and([x_ExplicitVarSizeWithFlags_Flags[q11] -> x_Occurrence[x_ExplicitVarSizeWithFlags_Values[q11]]
             | q11 : int(1..2)]),
    sum([toInt(x_Occurrence[q18]) | q18 : int(1..2)]) <=
    sum([toInt(x_ExplicitVarSizeWithFlags_Flags[q20]) | q20 : int(1..2)]),
    and([x_Occurrence[q15] ->
         or([x_ExplicitVarSizeWithFlags_Flags[q17] /\ x_ExplicitVarSizeWithFlags_Values[q17] = q15 | q17 : int(1..2)])
             | q15 : int(1..2)])

