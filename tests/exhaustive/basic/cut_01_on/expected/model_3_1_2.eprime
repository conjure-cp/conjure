    and([x_ExplicitVarSizeWithDummy[q6] = 4 -> x_ExplicitVarSizeWithDummy[q6 + 1] = 4 | q6 : int(1..2)]),
    and([x_ExplicitVarSizeWithDummy[q10] != 4 ->
         or([q12 <= x_ExplicitVarSizeWithMarker_Marker /\
             x_ExplicitVarSizeWithMarker_Values[q12] = x_ExplicitVarSizeWithDummy[q10]
                 | q12 : int(1..3)])
             | q10 : int(1..3)]),
    and([q14 <= x_ExplicitVarSizeWithMarker_Marker ->
         or([x_ExplicitVarSizeWithDummy[q16] != 4 /\
             x_ExplicitVarSizeWithDummy[q16] = x_ExplicitVarSizeWithMarker_Values[q14]
                 | q16 : int(1..3)])
             | q14 : int(1..3)]),
    and([x_ExplicitVarSizeWithDummy[q18] != 4 -> x_Occurrence[x_ExplicitVarSizeWithDummy[q18]] | q18 : int(1..3)]),
    and([x_Occurrence[q19] ->
         or([x_ExplicitVarSizeWithDummy[q21] != 4 /\ x_ExplicitVarSizeWithDummy[q21] = q19 | q21 : int(1..3)])
             | q19 : int(1..3)])

