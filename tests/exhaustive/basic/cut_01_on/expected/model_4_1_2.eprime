    and([x_ExplicitVarSizeWithDummy[q20] != 4 -> x_Occurrence[x_ExplicitVarSizeWithDummy[q20]] | q20 : int(1..3)]),
    and([x_Occurrence[q21] ->
         or([x_ExplicitVarSizeWithDummy[q23] != 4 /\ x_ExplicitVarSizeWithDummy[q23] = q21 | q23 : int(1..3)])
             | q21 : int(1..3)])

