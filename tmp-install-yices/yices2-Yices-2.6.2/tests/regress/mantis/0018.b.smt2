(set-logic QF_UF)
(declare-sort var 0)
(declare-sort reg 0)
(declare-fun a1_1 () var)
(declare-fun r0 () reg)
(declare-fun r1 () reg)
(declare-fun r2 () reg)
(declare-fun r3 () reg)
(declare-fun assign (var) reg)

(assert (distinct r0 r1 r2 r3))
(assert (or (= (assign a1_1) r3) (= (assign a1_1) r0) (= (assign a1_1) r1) (= (assign a1_1) r2)))
(assert (= (assign a1_1) r3))

(check-sat)
(exit)
